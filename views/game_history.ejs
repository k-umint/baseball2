<body>

    <%- include("components/header"); %>

        <div id="main">
            <!-- <div class="container"> -->
            <!-- <div id="green-field" class="row justify-content-center align-self-center bg-success"> -->
            <div id="green-field" class="row bg-success">
                <div id="brown-field" class="col border border-white m-3">
                    <div class="list-group">
                        <div id="scroll-container">
                            <% responseBody.gamesList.forEach(element=> { %>
                                <%- include("components/pastGames", {element : element}); %>
                                    <% }) %>
                        </div>
                    </div>
                    <a href="/" id="goHomeBtn" class="btn btn-warning">HOME</a>
                </div>
            </div>
        </div>

        <!-- Optional JavaScript -->
